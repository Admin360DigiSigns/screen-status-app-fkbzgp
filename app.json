
{
  "expo": {
    "name": "360DigiSigns",
    "slug": "360DigiSigns",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ded86abe-6a7d-491d-80a5-adc8948ee47e.jpeg",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/ded86abe-6a7d-491d-80a5-adc8948ee47e.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.digisigns.digisignsapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app needs camera access for WebRTC screen sharing",
        "NSMicrophoneUsageDescription": "This app needs microphone access for WebRTC screen sharing"
      },
      "bitcode": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ded86abe-6a7d-491d-80a5-adc8948ee47e.jpeg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.digisigns.digisignsapp",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "WAKE_LOCK",
        "CAMERA",
        "RECORD_AUDIO",
        "MODIFY_AUDIO_SETTINGS",
        "BLUETOOTH",
        "BLUETOOTH_CONNECT"
      ]
    },
    "web": {
      "favicon": "./assets/images/ded86abe-6a7d-491d-80a5-adc8948ee47e.jpeg",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "360DigiSigns",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  }
}
